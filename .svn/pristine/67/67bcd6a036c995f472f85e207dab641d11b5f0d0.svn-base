		}
		.user_form .people_list a {
		    line-height: 30px;
		    color: #14a6a3;
		    display: block;
		}
		.user_form .people_list a i {
		    height: 30px;
		    width: 25px;
		    display: block;
		    float: left;
		    margin: 5px 2px 5px 0px;
		    border-radius: 50%;
