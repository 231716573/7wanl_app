	//验证抵扣券
	function yzQkCode(){
		var coupon_qk_li_input = $api.val($api.dom(".coupon_qk_li_input"));
		api.ajax({
	        url:'http://192.168.19.239/dataapi.php?mod=coupon&action=coupon_apply_code',
	        method:"post",
	        data: {
		        values:{
		        	"code":coupon_qk_li_input,
		        	"c_type":1,
		        	"activity_id":GetString("activity_id"),
		        	"adult_num":GetString("adultnumber"),
		        	"child_num":GetString("childnumber"),
		        	"amount":GetString("totalprice"),
		        	"range":"app"
		        }
		    },
	        cache:true
        },function(ret,err){
        	if(ret){
        	
        		if(ret.code == -1){
        			alert(ret.msg)
        		}else{
        			
        			alert("优惠券面值"+ret.data.price_value+"元")
        			$api.html($api.dom(".check_coupon_code_qk"), "取消");
        			$api.addCls($api.dom(".check_coupon_code_qk"), "qk_price");
        			$api.removeCls($api.dom(".qk_price"), "check_coupon_code_qk");
        			$api.attr($api.dom(".coupon_qk_li_input"), "disabled","disabled");
        			if( ret.data.price_value - parseFloat($api.html($api.byId("totalprice"))) <= 0 ){
        				$api.html($api.byId("totalprice"), 0)
        			}else{
//      				alert("111")
        				$api.html($api.byId("totalprice"), (ret.data.price_value - parseFloat($api.html($api.byId("totalprice")))))
        			}
        		}
        	}
        	if(err){
        		alert(err.msg)
        	}
        });
	}
	
